         (create-component  True)
         (create-on-remote-object  False)
         (near-distance  25.000000)
         (at-distance  1.000000)
         (approach-speed  2.000000)
         (road-center-offset  2.000000)
         (weapon-process-state-repository-name  "m60-process-state")
