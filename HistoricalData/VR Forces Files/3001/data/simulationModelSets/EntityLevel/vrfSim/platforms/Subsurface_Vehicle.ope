      (sector-size  1.570796)
   )
   (task-execution-rules 
      (filename  "$(taskRules-dir)/default-task-rules.tsk")
   )
   (can-pivot False)
   (max-pivot-speed  0)
   (left-support  $left-support)
   (right-support  $right-support)
   (other-support  $other-support)
   (max-speed  $max-speed)
   (max-reverse-speed  10)
   (turning-radius  $turning-radius)
   (max-slope  $max-slope)
   (ordered-speed  $ordered-speed)
   (max-acceleration  $max-acceleration)
   (max-deceleration  $max-deceleration)
   (fuel-efficiency  10.0) ; Speed at which fuel-range in movement sysdef is valid
   (max-lateral-acceleration $max-lateral-acceleration)
   (max-depth $max-depth)
   (use-object-geometry $use-object-geometry (default true))
