         (destination-category "target-location-acceptor")
         (destination-port-name "target-location")
      )
   )
   (mass  $mass)
   (sensor-signatures 
      (visual 
         (base-signature  $visual-signature)
         (modifiers 
            (file 
               (external-file 
                  (filename  "$(signatureRules-dir)\subsurface-visual-illumination-rules.mtl")
               )
            )
         )
      )
      (radar 
         (base-signature  $radar-signature)
         (modifiers 
            (file 
               (external-file 
                  (filename  "$(signatureRules-dir)\subsurface-radar-illumination-rules.mtl")
               )
            )
         )
      )
	  (passive-sonar
	     (base-signature $passive-sonar-signature)
		 (modifiers 
            (file 
               (external-file 
                  (filename  "$(signatureRules-dir)\underwater-acoustics-rules.mtl")
               )
            )
		 )
	  )
      (active-sonar
         (base-signature $active-sonar-signature)
         (modifiers )
      )
	  (magnetic-anomaly-detection
         (base-signature  $mad-signature)
         (modifiers 
; Instead of these rules, which reduce signature based on depth, use just
; a detection table--which will give detection results based on total
; range through air and water. The table should reflect a detection capability
; that falls off a range^3. See
; http://www.fas.org/man/dod-101/navy/docs/fun/part09.htm.
; Other effects can be modeled in the future by putting modifiers in this file.
;            (file 
;               (external-file 
;                  (filename  "$(signatureRules-dir)\subsurface-mad-rules.mtl")
;               )
;            )
         )
      )
   )
   (engagement-rules  "fire-at-will")
   (resources )
   (state-repository-extension-type  "default-vrf-state-repository-user-extension")
   (tasked-by-superior-upon-task-complete True)
   (initial-sector-of-responsibility 
