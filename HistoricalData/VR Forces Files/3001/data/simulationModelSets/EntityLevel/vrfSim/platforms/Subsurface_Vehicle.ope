         (create-component $can-embark)
         (response-timeout  1.000000)
         (response-retries  2)
      )
      (event-trigger-controller 
         (component-descriptor-type "event-trigger-controller-descriptor")
         (component-type  "event-trigger-controller")
         (min-tick-period  10.000000)
         (min-tick-period-variance  -1.000000)
         (tick-period-uses-real-time False)
         (process-state-repository-name  "")
         (process-state-repository-type  "")
         (is-enabled True)
		 (detection-level-to-trigger  3)
         (object-types-to-detect 
            (object-type  1 (16 0 0 42 0 0 1))
         )
      )
   )
   (actuators )
   (connections )
   (auto-connections 
      (auto-connection
         (source-controller "event-trigger-controller")
         (source-port-name "object-types-to-detect")
         (destination-category "sensor")
         (destination-port-name "object-types-to-detect")
      )
      (auto-connection
         (source-category "sensor")
         (source-port-name "detected-objects")
         (destination-controller "event-trigger-controller")
         (destination-port-name "detected-objects")
      )
      (auto-connection
         (source-controller "target-selection")         
         (source-port-name "object-types-to-detect")
         (destination-category "sensor")
         (destination-port-name "object-types-to-detect")
      )
      (auto-connection
         (source-category "sensor")         
         (source-port-name "detected-objects")
         (destination-controller "contact-fusion")
         (destination-port-name "detected-objects")
      )
	  (auto-connection
         (source-category "other")
         (source-port-name "detected-objects")
         (destination-controller "contact-fusion")
         (destination-port-name "detected-objects")
