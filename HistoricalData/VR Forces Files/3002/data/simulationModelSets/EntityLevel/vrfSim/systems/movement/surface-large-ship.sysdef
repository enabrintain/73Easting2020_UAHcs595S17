		 (power-plant-active True)
      )
   )
   (actuators 
      (surface-entity-actuator 
         (component-descriptor-type "surface-entity-component-descriptor")
         (component-type  "surface-entity-simple-actuator")
         (min-tick-period  0.050000)
         (min-tick-period-variance  0.010000)
         (tick-period-uses-real-time False)
         (process-state-repository-name  "")
         (process-state-repository-type  "")
         (is-enabled True)
         (create-component True)
         (art-part-list )
         (max-yaw-acceleration  0.000000)
         (max-yaw-rate  0.000000)
         (check-soil-type True)
         (stopping-speed  0.500000)
         (turn-roll-rate  0.400000)
         (speed-pitch-rate  0.001500)
         (sinking-time 0)
      )
      (update-repository 
         (component-descriptor-type "actuator-component-descriptor")
         (component-type  "update-repository-actuator")
         (min-tick-period  -1.000000)
         (min-tick-period-variance  -1.000000)
         (tick-period-uses-real-time False)
         (process-state-repository-name  "")
         (process-state-repository-type  "")
         (is-enabled True)
         (create-component True)
         (art-part-list )
      )
   )
   (connections 
      (connect scripted-movement:repository-update update-repository:repository-update)
      (connect stop:surface-control surface-entity-actuator:surface-control)
      (connect turn-move:surface-control surface-entity-actuator:surface-control)
      (connect move-to:surface-control surface-entity-actuator:surface-control)
      (connect move-to-location:surface-control surface-entity-actuator:surface-control)
      (connect move-along:surface-control surface-entity-actuator:surface-control)
      (connect patrol-route:surface-control surface-entity-actuator:surface-control)
      (connect patrol-between-points:surface-control surface-entity-actuator:surface-control)
      (connect follow-entity:surface-control surface-entity-actuator:surface-control)
      (connect surface-entity-actuator:repository-update update-repository:repository-update)
   )
   (resources 
      (fuel 
         (resource-type "real-resource")
         (amount $fuel-amount)
         (full-amount $fuel-amount)
      )
   )
   (meta-data 
      (system-name  "Large Ship")
      (system-description  "Movement dynamics for a large ship.")
      (allowed-state-repository-types "surface-entity-param")
      (system-categories "movement")
      (parameter-data-list 
         (real-parameter-data 
            (parameter-name  "fuel-amount")
            (variable-type  "DtRwReal")
            (display-label  "Nominal movement range at nominal speed (see fuel factor parameter)")
            (display-units  "kilometers")
            (source-units  "kilometers")
            (default-value  1000.000000)
         )
         (real-parameter-data 
            (parameter-name  "normal-acceleration-fraction")
            (variable-type  "DtRwReal")
            (display-label  "Fraction of max acceleration used in normal operation")
            (display-units  "fraction of max acceleration")
            (source-units  "fraction of max acceleration")
            (default-value  0.700000)
         )
      )
      (meta-data-entry-list )
   )
)
